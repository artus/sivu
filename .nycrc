{
  "cache": false,
  "check-coverage": true,
  "extension": [
    ".ts"
  ],
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "**/index.ts",
    "coverage/**",
    "node_modules/**",
    "**/*.d.ts",
    "**/*.test.ts"
  ],
  "sourceMap": true,
  "reporter": [
    "text",
    "text-summary"
  ],
  "all": true,
  "instrument": true,
  "branches": 100,
  "lines": 100,
  "functions": 100,
  "statements": 100
}